{
    "name": "developapa",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "process:content": "node ./scripts/process-markdown.js",
        "build": "npm run process:content && ng run developapa:build:ssr,production",
        "serve:ssr": "node dist/developapa/server/server.mjs",
        "dev:ssr": "npm run process:content && ng run developapa:serve",
        "prerender": "ng run developapa:build:prerender,production && mv dist/developapa/browser/index.csr.html dist/developapa/browser/index.html",
        "serve:prerender": "http-server dist/developapa/browser -p 8080",
        "deploy": "npm run process:content && npm run prerender && cp _redirects dist/developapa/browser/_redirects"
    },
    "private": true,
    "description": "Personal blog",
    "author": "Nicolas Gehlert <info@ngehlert.de>",
    "bugs": {
        "url": "https://github.com/ngehlert/developapa/issues"
    },
    "dependencies": {
        "@angular/cdk": "21.1.1",
        "@angular/common": "21.1.2",
        "@angular/compiler": "21.1.2",
        "@angular/core": "21.1.2",
        "@angular/forms": "21.1.2",
        "@angular/material": "21.1.1",
        "@angular/platform-browser": "21.1.2",
        "@angular/platform-browser-dynamic": "21.1.2",
        "@angular/platform-server": "21.1.2",
        "@angular/router": "21.1.2",
        "@angular/ssr": "21.1.2",
        "@ngehlert/event-helper": "1.0.3",
        "@sentry/angular": "10.37.0",
        "ag-grid-angular": "33.0.4",
        "ag-grid-community": "33.0.4",
        "express": "4.22.1",
        "gray-matter": "4.0.3",
        "marked": "15.0.7",
        "material-design-icons": "3.0.1",
        "prismjs": "1.30.0",
        "rxjs": "7.8.0",
        "tslib": "2.3.0",
        "zone.js": "0.15.0"
    },
    "devDependencies": {
        "@angular/build": "21.1.2",
        "@angular/cli": "21.1.2",
        "@angular/compiler-cli": "21.1.2",
        "@types/express": "4.17.17",
        "@types/jasmine": "5.1.0",
        "@types/marked": "6.0.0",
        "@types/node": "18.18.0",
        "@types/prismjs": "1.26.5",
        "clear": "0.1.0",
        "dotenv": "17.2.1",
        "handlebars": "4.7.8",
        "http-server": "14.1.1",
        "jasmine-core": "5.6.0",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.0",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "netlify": "23.14.0",
        "prettier": "3.5.3",
        "prompt": "1.3.0",
        "typescript": "5.9.3"
    }
}
