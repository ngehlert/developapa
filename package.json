{
    "name": "developapa",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "process:content": "node ./scripts/process-markdown.js",
        "build": "npm run process:content && ng run developapa:build:ssr,production",
        "serve:ssr": "node dist/developapa/server/server.mjs",
        "dev:ssr": "npm run process:content && ng run developapa:serve",
        "prerender": "npm run process:content && ng run developapa:build:prerender,production && mv dist/developapa/browser/index.csr.html dist/developapa/browser/index.html",
        "serve:prerender": "http-server dist/developapa/browser -p 8080"
    },
    "private": true,
    "description": "Personal blog",
    "author": "Nicolas Gehlert <info@ngehlert.de>",
    "bugs": {
        "url": "https://github.com/ngehlert/developapa/issues"
    },
    "dependencies": {
        "@angular/common": "19.2.5",
        "@angular/compiler": "19.2.5",
        "@angular/core": "19.2.5",
        "@angular/forms": "19.2.5",
        "@angular/platform-browser": "19.2.5",
        "@angular/platform-browser-dynamic": "19.2.5",
        "@angular/platform-server": "19.2.5",
        "@angular/router": "19.2.5",
        "@angular/ssr": "19.2.6",
        "express": "4.21.2",
        "gray-matter": "4.0.3",
        "marked": "15.0.7",
        "prismjs": "1.30.0",
        "rxjs": "7.8.0",
        "tslib": "2.3.0",
        "zone.js": "0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "19.2.6",
        "@angular/cli": "19.2.6",
        "@angular/compiler-cli": "19.2.5",
        "@types/express": "4.17.17",
        "@types/jasmine": "5.1.0",
        "@types/marked": "6.0.0",
        "@types/node": "18.18.0",
        "@types/prismjs": "1.26.5",
        "http-server": "14.1.1",
        "jasmine-core": "5.6.0",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.0",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "prettier": "3.5.3",
        "typescript": "5.7.2"
    }
}
