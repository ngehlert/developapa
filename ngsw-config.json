{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/*.(png|jpg|jpeg|gif|svg|ico|webp)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "api-freshness",
            "urls": ["/**"],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 100,
                "maxAge": "1d",
                "timeout": "5s"
            }
        }
    ]
}
